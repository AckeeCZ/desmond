{
  "name": "tools",
  "description": "tools",
  "version": "0.0.0",
  "author": "Pan Unicorn <pan.unicorn@ackee.cz>",
  "engines": {
    "node": "^8.3.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "npm run build && NODE_PATH=src jest",
    "test:coverage": "npm run test -- --collectCoverage",
    "lint": "tslint -t codeFrame -p .",
    "docs": "typedoc --includeDeclarations --excludeExternals --mode file --theme markdown --out docs src/lib src/*.d.ts",
    "check": "npm-check -i app -i errors -i config -i index & exit 0"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "bcrypt": "^3.0.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.snakecase": "^4.1.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "jest": "^23.6.0",
    "npm-check": "^5.8.0",
    "ts-jest": "^23.10.4",
    "tslint-config-ackee": "^0.1.1",
    "typedoc": "^0.13.0",
    "typedoc-plugin-markdown": "^1.1.17",
    "typescript": "^3.1.2"
  }
}
